<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DashBoardAdmin.GraphsInfoTableComponent.GraphInfoTableController">
   <content>
      <TableView fx:id="graphInfoTable" minHeight="500.0" minWidth="800.0">
         <columns>
            <TableColumn fx:id="graphNameCol" prefWidth="108.0" text="Graph name" />
            <TableColumn fx:id="uploadedByCol" prefWidth="113.0" text="Uploaded By" />
            <TableColumn fx:id="totalTargetsCol" prefWidth="112.0" text="Total targets" />
            <TableColumn fx:id="independentCol" prefWidth="111.0" text="Independent" />
            <TableColumn fx:id="leafCol" prefWidth="75.0" text="Leaf" />
            <TableColumn fx:id="middleCol" prefWidth="75.0" text="Middle" />
            <TableColumn fx:id="rootCol" prefWidth="75.0" text="Root" />
            <TableColumn fx:id="pricesCol" prefWidth="87.0" text="Prices" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
