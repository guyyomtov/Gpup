<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tableForProcess.TableForProcessController">
   <content>
      <TableView fx:id="tableForProcess" minHeight="300.0" minWidth="600.0" prefHeight="323.0" prefWidth="600.0">
         <columns>
            <TableColumn fx:id="targetNameColumn" prefWidth="106.0" text="Target name" />
            <TableColumn fx:id="levelColumn" prefWidth="88.0" text="Level" />
            <TableColumn fx:id="statusColumn" prefWidth="83.0" text="Status" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
