<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tableView.TableController">
   <content>
      <TableView fx:id="tableView" minHeight="500.0" minWidth="800.0">
         <columns>
            <TableColumn fx:id="selectColumn" prefWidth="40.0">
               <graphic>
                  <CheckBox fx:id="selectAllCheckBox" mnemonicParsing="false" onAction="#selectAllCheckBoxAction" prefHeight="21.0" prefWidth="102.0" text="Select All" />
               </graphic></TableColumn>
            <TableColumn fx:id="targetNameColumn" prefWidth="86.0" text="Target name" />
            <TableColumn fx:id="levelColumn" prefWidth="88.0" text="Level" />
            <TableColumn fx:id="dependsOnColumn" prefWidth="73.0" text="Total &#10;depends on" />
            <TableColumn fx:id="requiredForColumn" prefWidth="107.0" text="Total &#10;required for&#10;" />
            <TableColumn fx:id="infoColumn" prefWidth="83.0" text="Info" />
            <TableColumn fx:id="serialSetColumn" prefWidth="111.0" text="Num of &#10;serial sets" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </content>
</ScrollPane>
